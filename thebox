    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        String deviceName, identify;
        int identifyNum;
        identify = deviceBox.getText();
        deviceName = aident(identify);
        
        output.setText(deviceName);
        
    }                                        


    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        
        //declaration of arrays/matrix
        int list[][] ={
            {55,20,5},//4s
            {100,30,10},//5
            {95,30,10},//5c
            {170,60,15},//5s
            {280,90,25},//6
            {320,110,30},//6Plus
            {90,20,10},//iPad 2
            {100,20,10},//iPad 3
            {120,30,10},//iPad 4
            {95,20,10},//iPad mini
            {110,30,10},//iPad mini 2/ retina
            {160,45,15},//iPad air
            {200,45,20},//iPad air 2
            {30,10,5},//iPod Touch 4th Gen
            {75,15,5}//iPod Touch 5th Gen
        };
        
        double carrierMod[] = new double[7];
        carrierMod[0] = .90; //AT&T
        carrierMod[1] = 1; //Verizon
        carrierMod[2] = .66; //Sprint
        carrierMod[3] = .75; //T-Mobile
        carrierMod[4] = .9;//WiFi Only
        carrierMod[5] = .66;//Other Carrier
        carrierMod[6] = 1;//Unlocked
        
        //collection of array data
        int arraypassX, arraypassY, arraypassZ;
        arraypassX = baseDevice.getSelectedIndex();
        arraypassY = baseCondition.getSelectedIndex();
        arraypassZ = carrierComboBox.getSelectedIndex();
        
        //modification of carrierMods
        if (arraypassX >= 6){ //if device is an iPad
            carrierMod[0] = 1;
            carrierMod[1] = .9;
            carrierMod[2] = .8;
            carrierMod[3] = .8;
        }
        //assignment of variables based on arrays
        double carrierValue = carrierMod[arraypassZ];
        double total = list[arraypassX][arraypassY]; //note conversion to double
        BigDecimal rt = new BigDecimal(total);
        BigDecimal coc = new BigDecimal(1.1);
        BigDecimal cable = new BigDecimal(5);
        BigDecimal cv = new BigDecimal(carrierValue);
        BigDecimal engrave = new BigDecimal(0.9);
        BigDecimal cashPenalty = new BigDecimal(0.7);
        BigDecimal hiCap = new BigDecimal(1.1);
        BigDecimal wCashPenalty;
        //calculation of data
        
        rt = rt.multiply(cv);
        //Chamber of Commerce Mod Checkbox
        if (cocMod.isSelected() == true){
            rt = rt.multiply(coc);
        }
        //Cable included checkbox
        if (cableMod.isSelected() == true){
            rt = rt.add(cable);
        }
        
        //Engraved checkbox
        if (engraveMod.isSelected() == true){
            rt = rt.multiply(engrave);
        }
        if (hiCapMod.isSelected() == true){
            rt = rt.multiply(hiCap);
        }
        wCashPenalty = rt.multiply(cashPenalty);
        NumberFormat n = NumberFormat.getCurrencyInstance(Locale.US);
        String storeCredit = n.format(rt);
        String cashValue = n.format(wCashPenalty);
        storeCreditField.setText(String.valueOf(storeCredit));
        cashField.setText(String.valueOf(cashValue));
    }                                        

